ras\">\r\n            {extras.map((item, index) => {\r\n              return <li key={index}> -{item}</li>;\r\n            })}\r\n          </ul>\r\n        </section>\r\n      </>\r\n    );\r\n  }\r\n}\r\n","C:\\OTHERS\\Codes\\beach-resort-\\src\\Error.js",[],[],"C:\\OTHERS\\Codes\\beach-resort-\\src\\data.js",["116"],[],"C:\\OTHERS\\Codes\\beach-resort-\\src\\Contentful.js",[],[],"C:\\OTHERS\\Codes\\beach-resort-\\src\\Components\\Navbar.js",[],[],"C:\\OTHERS\\Codes\\beach-resort-\\src\\Components\\Services.js",["117"],[],"C:\\OTHERS\\Codes\\beach-resort-\\src\\Components\\Banner.js",[],[],"C:\\OTHERS\\Codes\\beach-resort-\\src\\Components\\RoomsContainer.js",[],[],"C:\\OTHERS\\Codes\\beach-resort-\\src\\Components\\FeaturedRoom.js",[],[],"C:\\OTHERS\\Codes\\beach-resort-\\src\\Components\\Hero.js",[],[],"C:\\OTHERS\\Codes\\beach-resort-\\src\\Components\\StyledHero.js",[],[],"C:\\OTHERS\\Codes\\beach-resort-\\src\\Components\\loading.js",[],[],"C:\\OTHERS\\Codes\\beach-resort-\\src\\Components\\Title.js",[],[],"C:\\OTHERS\\Codes\\beach-resort-\\src\\Components\\Room.js",[],[],"C:\\OTHERS\\Codes\\beach-resort-\\src\\Components\\RoomsFilter.js",[],[],"C:\\OTHERS\\Codes\\beach-resort-\\src\\Components\\RoomsList.js",[],[],{"ruleId":"118","severity":1,"message":"119","line":3,"column":8,"nodeType":"120","messageId":"121","endLine":3,"endColumn":14},{"ruleId":"118","severity":1,"message":"122","line":3,"column":8,"nodeType":"120","messageId":"121","endLine":3,"endColumn":12},{"ruleId":"123","severity":1,"message":"124","line":18,"column":1,"nodeType":"125","endLine":733,"endColumn":3},{"ruleId":"118","severity":1,"message":"126","line":3,"column":22,"nodeType":"120","messageId":"121","endLine":3,"endColumn":30},"no-unused-vars","'Client' is defined but never used.","Identifier","unusedVar","'Hero' is defined but never used.","import/no-anonymous-default-export","Assign array to a variable before exporting as module default","ExportDefaultDeclaration","'FaHiking' is de